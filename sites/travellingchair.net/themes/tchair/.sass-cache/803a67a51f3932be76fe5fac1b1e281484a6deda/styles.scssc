/** Banner **/
.region-banner{
	float: left;
	height: 285px;
	margin: $nav (-$main) 0 0;
	width: $main;
	overflow: visible;
	word-wrap: break-word;
}

.banner #page #main .region-sidebar-first,
.banner #page #main .region-sidebar-second,
.banner #page #main #content {
	margin-top: $nav+20px+226px+20px;
}

/** NESTING STYLE **/


body{

	/* HTML ELEMENTS */
	color: $body-color;	
	
	@include linear-gradient(color-stops(#BBE3F4, #e1f3fa));
	
	background-color: #e1f3fa;
	background-repeat: repeat-x;
	
	h1{
		@include h1;
	}

	h2{
		@include h2;
	}

	h3{
		@include h3;
	}

	h4, h5, h6{
		@include h4;
	}

	p, ul, ol, dl, pre, table, fieldset, blockquote {
		margin:1em 0;
	}

	/* LINK */
	a{
		color: $links-color;
		outline: none;
		text-decoration: none;
	}
	
	
	/* Tinymce Style */

	.caption {
		background: #ddd;
		font-size: smaller;
		text-align: center;
	}		
	
	
	//Hide location
	&.node-type-location #page-wrapper #main #content{
		h1#page-title{
			display: none;
		}
	}
	&.node-type-location.page-node-edit #page-wrapper #main #content{
		h1#page-title{
			display: block;
		}
		
	}
	
	//hide tabs for not admin users
	&.page-user .tabs,
	&.node-type-location.logged-in .tabs{
		display: none;
	}
	&.page-user.logged-in.admin-menu .tabs,
	&.node-type-location.logged-in.admin-menu .tabs{
		display: block;
	}
	
	//login & create account page
	&.not-logged-in.page-user .tabs{
	//	display: none;
	}
	
	
	//longer group-teaser-left if no sidebar
	&.no-sidebars #page-wrapper #main #content .node-location.node-teaser .group-teaser-left{
		width: 550px;
	}
		
#page-wrapper{

	@include border-radius(20px, 20px);
	margin: 30px auto;
	background: #fff;

	/* Block */
	.block{
		h2.block-title{
			@include roundbox;
		}
		
		.content{
			padding: 10px 15px;
		}
		
		//My Reviews block
		&#block-views-user-reviews-block .content{
			padding: 0;
		}

		.node .content{
			padding: 0;

			p{
				margin-top: 0;
			}
		}
	}

		
#header{
		
	#logo{
	}

	h1#site-name, div#site-name{
	
		a span{
			display: block;
			width: 289px;
			margin-top: 20px;
			@include replace-text("../images/site-name.gif");
		}
		
	}
	
	#site-slogan{
		@include replace-text("../images/site-slogan.gif");
	}
}
		
#main{
			
#navigation{


	#block-block-5{
		display: none;
	}
	
	.block{
		
		float: left;
		margin-bottom: 0px;

		.content{
			padding: 0;
		}	

		ul{
			
			li{
				margin: 0px;
			}
		
			&:first-child li {
				a{
					@include roundbox($blue, 148px);
				}

				&.first a{
					@include replace-text("../images/home.gif");
					width: 6px;
				}	

				&.last{
					padding-right: 0px;
				}
			}
		}

		
		/* Login out buttons */
		&#block-nodeblock-2 {
			float: right;
			@include login-out($orange);
		
		}
		&#block-nodeblock-50 {
		
			float: right;
			@include login-out($blue);
			
			ul li:first-child a{
				width: 116px;

			}
			

		}
	}
	

	
	
			
} //#navigation

.region-banner{
	
	.block .content{
		padding: 0;
	}
	
	//Banner 
	.view-banner{
		@include clearfix;
		height: 226px;
		
		@include border-radius;	
		
		background-color: #D9EAF5;
		
		.views-field-field-image{
			float: left;
			
			img{
				@include border-right-radius;	
			}
		}
		
		
		.views-field-title{
			margin-left: 600+20px;
			padding: 20px 20px 0 20px;
			color: $blue;
			@include h2;
			text-transform: uppercase;
			margin-top: 0;
		}
		
		.views-field-edit-node{
			margin-left: 600+20px;
			margin-top: -1em;
			padding: 0px 20px;	
			display: none;	
			position: absolute;	
		}
		
		&:hover .views-field-edit-node{
			display: block;
		}
		
		.views-field-body{
			margin-left: 600+20px;
			padding: 0px 20px;
			color: #3890CB;
		}
	}
	
} //.region-banner
			
#content{

	.section{
		position: relative;
	}

	.submitted {
		font-size: smaller;
	}
	
	#page-title{
		@include roundbox;
	}	
	
	
	/* Location */
	& .node-location{

		h3.location-locations-header{
			display: none;
		}
		
		// Location
		.group-location{
			position: relative;
			
			.field-name-field-edit{
				position: absolute;
				top: 0;
				right: 0;
				
				a.button{
					@include title-button;
				}
				
			}
		
			margin-bottom: 2em;		
			display: none;
		
			.description{
				@include roundbox;
			}
			
			.group-location-content{
				background: #F2F2F2;
				padding: 10px;
				
				@include clearfix;

				// Map (field latitude)
				.field-name-field-latitude{
					float: right;
					margin: 0 0 10px 10px;
				}
				
				// Title field
				.field-name-field-title{
					@include h1;
					color: $green;
					text-transform: capitalize;
					font-weight: bold;
					
					float: left;
					width: 280px;
				}

				
				//Subheading
				.field-name-field-subheading{
					margin-bottom: 1em;
					font-weight: bold;
				}
				
				//Address
				.field-name-field-address{
					font-size: 1.2em;
				}
			}
		}
		
		//Review
		.group_review {
			
			.description{
				//@include roundbox;
				display: none;
			}
		
			// Review now		
			.field-name-field-review-now {
				@include clearfix;
				
				.title{
					@include h1;
					color: $green;
					clear: both;
					text-transform: capitalize;
					font-weight: bold;
				}
				
				a{
					&.review, &.login{
						@include roundbox-style($blue, 210px);					
						@include border-left-radius();
											
						background: {
							image: url(../images/blue-arrow-right.gif);
							position: right center;
							repeat: no-repeat;
						}
					}
					
				}

			}

		} // .group_review
		
					
		a.result{
			@include roundbox-style($blue, 150px);	
			@include border-right-radius();
			text-align: right;
			float: right;
						
			background: {
				image: url(../images/blue-arrow-left.gif);
				position: left center;
				repeat: no-repeat;
			}
		}

		// VISUAL, HEARING, MOBILITY
		.group_visual, .group_hearing, .group_mobility{

			display: none;

			padding: 10px;

			// VISUAL, HEARING, MOBILITY title
			.description{
				text-transform: uppercase;
				border-bottom: 1px solid $border-color;
				font-weight: bold;
				@include h3;
			}

			div.field-type-number-float.shade{
				background: #F2F2F2;
				border-top: 1px solid $border-color;
				border-bottom: 1px solid $border-color;	
			}

			div.rate-field-label{
				float: left;
				line-height: 33px;
			}
		} //.group_visual, .group_hearing, .group_mobility

		/* Rate */
		.rate-widget-emotion {


			// Buttons
			.item-list{
				display: none;

				ul{
					margin: 0;
					padding-top: 4px;
					float: right;


					li{

					}
				}

				a{

					&.rate-button{
						height: 24px;
						width: 24px;		
						line-height: 24px;
						display: block;
						padding: 0;
						margin: 0 5px;
						text-align: center;
						text-decoration: none;	
						border-color: #ccc;
						background-color: #ddd;
						color: #666 !important;	
					}
					&.up{
						@include replace-text("../images/thumbs_up.gif", 0, 0);
						border: none;

						&:hover, &.active, &.selected{
							@include replace-text("../images/thumbs_up.gif", 0, -24px);
						}
					}

					&.down{
						@include replace-text("../images/thumbs_down.gif", 0, 0);
						border: none;

						&:hover, &.active, &.selected{
							@include replace-text("../images/thumbs_down.gif", 0, -24px);
						}
					}

					&.na{
						&:hover, &.active, &.selected{
							border-color: #1083d1;
							background-color: #BBE3F4;
							color: #1083d1 !important;
						}
					}
				}

			}

		} //.rate-widget-emotion


		// Percentage bar
		.group_visual .result{
			background: url(../images/icon_visual.gif) left center no-repeat;
		}
		.group_hearing .result{
			background: url(../images/icon_hearing.gif) left center no-repeat;
		}
		.group_mobility .result{
			background: url(../images/icon_mobility.gif) left center no-repeat;
		}
		
		.rate-info{
			display: none;
			display: inline;
			
			.result {

				float: right;
				width: 195px;
				line-height: 33px;
				padding-left: 25px;

				.percentage-bar {
					@include inline-block;
					background: #c00;
					width: 100px;
					height: 10px;

					&.empty{
						background-color: $border-color;
					}

					div.good{
						display: block;
						background: $green;	
						height: 10px;
					}
				}
			}
		}	//.rate-info
	
		//Location teaser
		&.node-teaser{
			@include clearfix;
			
			//category icon
			.category-icon{
				position: absolute;
			}
			
			.group-teaser-left{
				width: 250px;
				float: left;
				margin-left: 40px;
				margin-right: 10px;
				
				.field-name-field-title{
					@include h3;
					text-transform: capitalize;
					margin-top: 0px;
					margin-bottom: 0;
					font-weight: bold;
				}
				
				.field-name-field-subheading{
					line-height: 1.5em;
					
				}
				
				.field-name-field-address{
					font-size: .8em;
				}
			}
			
			.group-teaser-right {
				float: right;
				width: 262px;
				
				
				.rate-info{
					display: block;
					@include clearfix;
				}
				
				span.label{
					font-weight: bold;
					float: left;
					line-height: 33px;
					padding-left: 25px;
					
					&.visual{
						background: url(../images/icon_visual.gif) left center no-repeat;
					}
					&.hearing{
						background: url(../images/icon_hearing.gif) left center no-repeat;
					}
					&.mobility{
						background: url(../images/icon_mobility.gif) left center no-repeat;
					}
					
				}
				
				.result{
					padding-left: 0;
					width: auto;
				}
			}
		} //&.node-teaser
		
	} // & .node-location
	
	
	//Location submit form
	#location-node-form {
		padding: 0 10px;
		
		#edit-title{
		}
		
		#edit-field-address {
		
		}
		
		#edit-field-city, #edit-field-postcode{
			float: left;
			clear: left;
			input {
				width: 195px;
			}			
		}
		
		#edit-field-state, #edit-field-country{
			float: left;
			margin-left: 20px;
			input, select {
				width: 195px;
			}
			
			
		}
		
		.map {
			position: relative;
			clear: left;
			
			label{
				
				background-color: rgba(0, 0, 0, 0.66);
				color: #fff;
				font-weight: normal;
				padding: 2px 5px;
				width: 360px;
				@include border-radius(3px);
				position: absolute;
				top: 3px;
				left: 30px;
				z-index: 40;
			}
			
		}
		
		#create-location-map{
			clear: left;
		}
		
		.vertical-tabs{
			display: none;
		}
		
		#edit-field-latitude,
		#edit-field-longitude{
			display: none;
		}
		
	} //#location-node-form 


	//User login
	.rpx-links{
		border-bottom: 1px solid $border-color;
		
		span.ext{
			display: none;
		}
		
		// hide link "Sign in using one of these accounts:"
		br,
		a.rpxnow.ext:first-child{
			display: none;
		}
		

	}

	//User Registration
	#user-register-form{
		#field-user-city-add-more-wrapper{
			.descriptions{
				font-size: 10px;
