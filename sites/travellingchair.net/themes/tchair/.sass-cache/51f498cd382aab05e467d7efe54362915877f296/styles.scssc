			cursor: pointer;
			padding: 5px 17px;
			margin-bottom: 1em;
			margin-right: 1em;
			color: #5A5A5A;
			text-align: center;
			font-weight: normal;
			font-size: 1.077em;
			font-family: "Lucida Grande", Verdana, sans-serif;
			border: 1px solid #E4E4E4;
			border-bottom: 1px solid #B4B4B4;
			border-left-color: #D2D2D2;
			border-right-color: #D2D2D2;
			background: url(../images/buttons.png) 0 0 repeat-x;
			
			@include border-radius;
						
			&:active{
				background: #666;
				color: white;
				border-color: #555;
				text-shadow: #222 0 -1px 0;
				
			}
		}
		
		
	}
	
} //#content
			
.region-sidebar-first{
			
} //.region-sidebar-first

.region-sidebar-second{
	
	//Follows
	#block-views-my-friends-block{
		.views-row{
			@include clearfix;
			border-bottom: 1px solid $border-color;
			padding-bottom: 1em;
			margin-bottom: 1em;
			
		}
		.views-field-picture{
			float: left;
			margin-right: 10px;
		}
	}
	
	//Reviewed by
	#block-views-reviewed-by-block{
		.content{
			padding: 10px 0;
			
			.views-row{
				border-bottom: 1px solid $border-color;
				@include clearfix;
				margin-bottom: 10px;
				padding: 0 15px;
				padding-bottom: 10px;
				
				.avatar{
					float: left;
					margin-right: 10px;
				}

			}
		}

	}
			
} //.region-sidebar-second
		
} //#main

.region-footer{

	background: #F2F2F2;
	@include border-radius(20px);
	
	
	.block{
		margin: 0;
		margin-top: 1em;
		
		.content{
			padding: 0px 5px;
			color: #B3B3B3;
			
		}//.content
	}//.block
	
} //.region-footer

} //#page

} //body

