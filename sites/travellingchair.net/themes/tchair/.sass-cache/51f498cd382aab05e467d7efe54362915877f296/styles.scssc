#content{

	.section{
		position: relative;
	}

	.submitted {
		font-size: smaller;
	}
	
	#page-title{
		@include roundbox;
	}	
	
	
	/* Location */
	& .node-location{

		h3.location-locations-header{
			display: none;
		}
		
		// Location
		.group-location{
			position: relative;
			
			.field-name-field-edit{
				position: absolute;
				top: 0;
				right: 0;
				
				a.button{
					@include title-button;
				}
				
			}
		
			margin-bottom: 2em;		
			display: none;
		
			.description{
				@include roundbox;
			}
			
			.group-location-content{
				background: #F2F2F2;
				padding: 10px;
				
				@include clearfix;

				// Map (field latitude)
				.field-name-field-latitude{
					float: right;
					margin: 0 0 10px 10px;
				}
				
				// Title field
				.field-name-field-title{
					@include h1;
					color: $green;
					text-transform: capitalize;
					font-weight: bold;
					
					float: left;
					width: 280px;
				}

				
				//Subheading
				.field-name-field-subheading{
					margin-bottom: 1em;
					font-weight: bold;
				}
				
				//Address
				.field-name-field-address{
					font-size: 1.2em;
				}
			}
		}
		
		//Review
		.group_review {
			
			.description{
				//@include roundbox;
				display: none;
			}
		
			// Review now		
			.field-name-field-review-now {
				@include clearfix;
				
				.title{
					@include h1;
					color: $green;
					clear: both;
					text-transform: capitalize;
					font-weight: bold;
				}
				
				a{
					&.review, &.login{
						@include roundbox-style($blue, 210px);					
						@include border-left-radius();
											
						background: {
							image: url(../images/blue-arrow-right.gif);
							position: right center;
							repeat: no-repeat;
						}
					}
					
				}

			}

		} // .group_review
		
					
		a.result{
			@include roundbox-style($blue, 150px);	
			@include border-right-radius();
			text-align: right;
			float: right;
						
			background: {
				image: url(../images/blue-arrow-left.gif);
				position: left center;
				repeat: no-repeat;
			}
		}

		// VISUAL, HEARING, MOBILITY
		.group_visual, .group_hearing, .group_mobility{

			display: none;

			padding: 10px;

			// VISUAL, HEARING, MOBILITY title
			.description{
				text-transform: uppercase;
				border-bottom: 1px solid $border-color;
				font-weight: bold;
				@include h3;
			}

			div.field-type-number-float.shade{
				background: #F2F2F2;
				border-top: 1px solid $border-color;
				border-bottom: 1px solid $border-color;	
			}

			div.rate-field-label{
				float: left;
				line-height: 33px;
			}
		} //.group_visual, .group_hearing, .group_mobility

		/* Rate */
		.rate-widget-emotion {


			// Buttons
			.item-list{
				display: none;

				ul{
					margin: 0;
					padding-top: 4px;
					float: right;


					li{

					}
				}

				a{

					&.rate-button{
						height: 24px;
						width: 24px;		
						line-height: 24px;
						display: block;
						padding: 0;
						margin: 0 5px;
						text-align: center;
						text-decoration: none;	
						border-color: #ccc;
						background-color: #ddd;
						color: #666 !important;	
					}
					&.up{
						@include replace-text("../images/thumbs_up.gif", 0, 0);
						border: none;

						&:hover, &.active, &.selected{
							@include replace-text("../images/thumbs_up.gif", 0, -24px);
						}
					}

					&.down{
						@include replace-text("../images/thumbs_down.gif", 0, 0);
						border: none;

						&:hover, &.active, &.selected{
							@include replace-text("../images/thumbs_down.gif", 0, -24px);
						}
					}

					&.na{
						&:hover, &.active, &.selected{
							border-color: #1083d1;
							background-color: #BBE3F4;
							color: #1083d1 !important;
						}
					}
				}

			}

		} //.rate-widget-emotion


		// Percentage bar
		.group_visual .result{
			background: url(../images/icon_visual.gif) left center no-repeat;
		}
		.group_hearing .result{
			background: url(../images/icon_hearing.gif) left center no-repeat;
		}
		.group_mobility .result{
			background: url(../images/icon_mobility.gif) left center no-repeat;
		}
		
		.rate-info{
			display: none;
			display: inline;
			
			.result {

				float: right;
				width: 195px;
				line-height: 33px;
				padding-left: 25px;

				.percentage-bar {
					@include inline-block;
					background: #c00;
					width: 100px;
					height: 10px;

					&.empty{
						background-color: $border-color;
					}

					div.good{
						display: block;
						background: $green;	
						height: 10px;
					}
				}
			}
		}	//.rate-info
	
		//Location teaser
		&.node-teaser{
			@include clearfix;
			
			//category icon
			.category-icon{
				position: absolute;
			}
			
			.group-teaser-left{
				width: 250px;
				float: left;
				margin-left: 40px;
				margin-right: 10px;
				
				.field-name-field-title{
					@include h3;
					text-transform: capitalize;
					margin-top: 0px;
					margin-bottom: 0;
					font-weight: bold;
				}
				
				.field-name-field-subheading{
					line-height: 1.5em;
					
				}
				
				.field-name-field-address{
					font-size: .8em;
				}
			}
			
			.group-teaser-right {
				float: right;
				width: 262px;
				
				
				.rate-info{
					display: block;
					@include clearfix;
				}
				
				span.label{
					font-weight: bold;
					float: left;
					line-height: 33px;
					padding-left: 25px;
					
					&.visual{
						background: url(../images/icon_visual.gif) left center no-repeat;
					}
					&.hearing{
						background: url(../images/icon_hearing.gif) left center no-repeat;
					}
					&.mobility{
						background: url(../images/icon_mobility.gif) left center no-repeat;
					}
					
				}
				
				.result{
					padding-left: 0;
					width: auto;
				}
			}
		} //&.node-teaser
		
	} // & .node-location
	
	
	//Location submit form
	#location-node-form {
		padding: 0 10px;
		
		#edit-title{
		}
		
		#edit-field-address {
		
		}
		
		#edit-field-city, #edit-field-postcode{
			float: left;
			clear: left;
			input {
				width: 195px;
			}			
		}
		
		#edit-field-state, #edit-field-country{
			float: left;
