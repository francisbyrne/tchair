/** Banner **/
.region-banner{
	float: left;
	height: 285px;
	margin: $nav (-$main) 0 0;
	width: $main;
	overflow: visible;
	word-wrap: break-word;
}

.banner #page #main .region-sidebar-first,
.banner #page #main .region-sidebar-second,
.banner #page #main #content {
	margin-top: $nav+20px+226px+20px;
}

/** NESTING STYLE **/


body{

	/* HTML ELEMENTS */
	color: $body-color;	
	
	@include linear-gradient(color-stops(#BBE3F4, #e1f3fa));
	
	background-color: #e1f3fa;
	background-repeat: repeat-x;
	
	h1{
		@include h1;
	}

	h2{
		@include h2;
	}

	h3{
		@include h3;
	}

	h4, h5, h6{
		@include h4;
	}

	p, ul, ol, dl, pre, table, fieldset, blockquote {
		margin:1em 0;
	}

	/* LINK */
	a{
		color: $links-color;
		outline: none;
		text-decoration: none;
	}
	
	
	/* Tinymce Style */

	.caption {
		background: #ddd;
		font-size: smaller;
		text-align: center;
	}		
	
	
	//Hide location
	&.node-type-location #page-wrapper #main #content{
		h1#page-title{
			display: none;
		}
	}
	&.node-type-location.page-node-edit #page-wrapper #main #content{
		h1#page-title{
			display: block;
		}
		
	}
	
	//hide tabs for not admin users
	&.page-user .tabs,
	&.node-type-location.logged-in .tabs{
		display: none;
	}
	&.page-user.logged-in.admin-menu .tabs,
	&.node-type-location.logged-in.admin-menu .tabs{
		display: block;
	}
	
	//login & create account page
	&.not-logged-in.page-user .tabs{
	//	display: none;
	}
	
	
	//longer group-teaser-left if no sidebar
	&.no-sidebars #page-wrapper #main #content .node-location.node-teaser .group-teaser-left{
		width: 550px;
	}
		
#page-wrapper{

	@include border-radius(20px, 20px);
	margin: 30px auto;
	background: #fff;

	/* Block */
	.block{
		h2.block-title{
			@include roundbox;
		}
		
		.content{
			padding: 10px 15px;
		}
		
		//My Reviews block
		&#block-views-user-reviews-block .content{
			padding: 0;
		}

		.node .content{
			padding: 0;

			p{
				margin-top: 0;
			}
		}
	}

		
#header{
		
	#logo{
	}

	h1#site-name, div#site-name{
	
		a span{
			display: block;
			width: 289px;
			margin-top: 20px;
			@include replace-text("../images/site-name.gif");
		}
		
	}
	
	#site-slogan{
		@include replace-text("../images/site-slogan.gif");
	}
}
		
#main{
			
#navigation{


	#block-block-5{
		display: none;
	}
	
	.block{
		
		float: left;
		margin-bottom: 0px;

		.content{
			padding: 0;
		}	

		ul{
			
			li{
				margin: 0px;
			}
		
			&:first-child li {
				a{
					@include roundbox($blue, 148px);
				}

				&.first a{
					@include replace-text("../images/home.gif");
					width: 6px;
				}	

				&.last{
					padding-right: 0px;
				}
			}
		}

		
		/* Login out buttons */
		&#block-nodeblock-2 {
			float: right;
			@include login-out($orange);
		
		}
		&#block-nodeblock-50 {
		
			float: right;
			@include login-out($blue);
			
			ul li:first-child a{
				width: 116px;

			}
			

		}
	}
	

	
	
			
} //#navigation

.region-banner{
	
	.block .content{
		padding: 0;
	}
	
	//Banner 
	.view-banner{
		@include clearfix;
		height: 226px;
		
		@include border-radius;	
		
		background-color: #D9EAF5;
		
		.views-field-field-image{
			float: left;
			
			img{
				@include border-right-radius;	
			}
		}
		
		
		.views-field-title{
			margin-left: 600+20px;
			padding: 20px 20px 0 20px;
			color: $blue;
			@include h2;
			text-transform: uppercase;
			margin-top: 0;
		}
		
		.views-field-edit-node{
			margin-left: 600+20px;
			margin-top: -1em;
			padding: 0px 20px;	
			display: none;	
			position: absolute;	
		}
		
		&:hover .views-field-edit-node{
			display: block;
		}
		
		.views-field-body{
			margin-left: 600+20px;
			padding: 0px 20px;
			color: #3890CB;
		}
	}
	
