/** Banner **/
.region-banner{
	float: left;
	height: 285px;
	margin: $nav+20px (-$main) 0 0;
	width: $main;
	overflow: visible;
	word-wrap: break-word;
}

.banner #page #main .region-sidebar-first,
.banner #page #main .region-sidebar-second,
.banner #page #main #content {
	margin-top: $nav+20px+226px+20px;
}

/** NESTING STYLE **/


body{

	/* HTML ELEMENTS */
	color: $body-color;	
	
	@include linear-gradient(color-stops(#BBE3F4, #e1f3fa));
	
	background-color: #e1f3fa;
	background-repeat: repeat-x;
	
	h1{
		@include h1;
	}

	h2{
		@include h2;
	}

	h3{
		@include h3;
	}

	h4, h5, h6{
		@include h4;
	}

	p, ul, ol, dl, pre, table, fieldset, blockquote {
		margin:1em 0;
	}

	/* LINK */
	a{
		color: $links-color;
		outline: none;
		text-decoration: none;
	}
	
	
	/* Tinymce Style */

	.caption {
		background: #ddd;
		font-size: smaller;
		text-align: center;
	}		
	
	
	//Hide location
	&.node-type-location #page-wrapper #main #content{
		h1#page-title{
			display: none;
		}
	}
	&.node-type-location.page-node-edit #page-wrapper #main #content{
		h1#page-title{
			display: block;
		}
		
	}
	
	//hide tabs for not admin users
	&.page-user.logged-in .tabs,
	&.node-type-location.logged-in .tabs{
		display: none;
	}
	&.page-user.logged-in.admin-menu .tabs,
	&.node-type-location.logged-in.admin-menu .tabs{
		display: block;
	}
	
	//login & create account page
	&.not-logged-in.page-user .tabs{
	//	display: none;
	}
	
	
	//longer group-teaser-left if no sidebar
	&.no-sidebars #page-wrapper #main #content .node-location.node-teaser .group-teaser-left{
		width: 550px;
	}
	

		
#page-wrapper{

	@include border-radius(20px, 20px);
	margin: 30px auto;
	background: #fff;

	/* Block */
	.block{
		h2.block-title{
			@include roundbox;
		}
		
		.content{
			padding: 10px 0px;
		}
		
		//My Reviews block
		&#block-views-user-reviews-block .content{
			padding: 0;
		}

		.node .content{
			padding: 0;

			p{
				margin-top: 0;
			}
		}
	}

		
#header{
		
	#logo{
	}

	h1#site-name, div#site-name{
	
		a span{
			display: block;
			width: 289px;
			margin-top: 20px;
			@include replace-text("../images/site-name.gif");
		}
		
	}
	
	#site-slogan{
		@include replace-text("../images/site-slogan.gif");
	}
}
		
#main{
			
#navigation{


	#block-block-5{
		display: none;
	}
	
	.block{
		
		float: left;
		margin-bottom: 0px;

		.content{
			padding: 0;
		}	

		ul{
			
			li{
				margin: 0px;
			}
		
			&:first-child li {
				a{
					@include roundbox($blue, 148px);
				}

				&.first a{
					@include replace-text("../images/home.gif");
					width: 6px;
				}	

				&.last{
					padding-right: 0px;
				}
			}
		}

		
		/* Login out buttons */
		&#block-nodeblock-2 {
			float: right;
			@include login-out($orange);
		
		}
		&#block-nodeblock-50 {
		
			float: right;
			@include login-out($blue);
			
			ul li:first-child a{
				width: 116px;

			}
			

		}
	}
	

	
	
			
} //#navigation

.region-banner{
	
	.block .content{
		padding: 0;
	}
	
	//Banner 
	.view-banner{
		@include clearfix;
		height: 226px;
		
		@include border-radius;	
		
		background-color: #D9EAF5;
		
		.views-field-field-image{
			float: left;
			
			img{
				@include border-right-radius;	
			}
		}
		
		
		.views-field-title{
			margin-left: 600+20px;
			padding: 20px 20px 0 20px;
			color: $blue;
			@include h2;
			text-transform: uppercase;
			margin-top: 0;
		}
		
		.views-field-edit-node{
			margin-left: 600+20px;
			margin-top: -1em;
			padding: 0px 20px;	
			display: none;	
			position: absolute;	
		}
		
		&:hover .views-field-edit-node{
			display: block;
		}
		
		.views-field-body{
			margin-left: 600+20px;
			padding: 0px 20px;
			color: #3890CB;
		}
	}
	
} //.region-banner
			
#content{

	.section{
		position: relative;
	}

	.submitted {
		font-size: smaller;
	}
	
	#page-title{
		@include roundbox;
	}	
	
	
	/* Location */
	& .node-location{

		h3.location-locations-header{
			display: none;
		}
		
		// Location
		.group-location{
			position: relative;
			
			.field-name-field-edit{
				position: absolute;
				top: 0;
				right: 0;
				
				a.button{
					@include title-button;
				}
				
			}
		
			margin-bottom: 2em;		
			display: none;
		
			.description{
				@include roundbox;
			}
			
			.group-location-content{
				background: #F2F2F2;
				padding: 10px;
				
				@include clearfix;

				// Map (field latitude)
				.field-name-field-latitude{
					float: right;
					margin: 0 0 10px 10px;
				}
				
				// Title field
				.field-name-field-title{
					@include h1;
					color: $green;
					text-transform: capitalize;
					font-weight: bold;
					
					float: left;
					width: 280px;
				}

				
				//Subheading
				.field-name-field-subheading{
					margin-bottom: 1em;
				}
			}
		}
		
		//Review
		.group_review {
			
			.description{
				//@include roundbox;
				display: none;
			}
		
			// Review now		
			.field-name-field-review-now {
				@include clearfix;
				
				.title{
					@include h1;
					color: $green;
					clear: both;
					text-transform: capitalize;
					font-weight: bold;
				}
				
				a{
					@include roundbox-style($blue, 210px);				
					
					&.result{
						@include roundbox-style($blue, 150px);	
						@include border-right-radius();
						text-align: right;
						float: right;
						
						background: {
							image: url(../images/blue-arrow-left.gif);
							position: left center;
							repeat: no-repeat;
						}
					}
					&.review, &.login{
						@include border-left-radius();
											
						background: {
							image: url(../images/blue-arrow-right.gif);
							position: right center;
							repeat: no-repeat;
						}
					}
					
				}

			}

		} // .group_review

		// VISUAL, HEARING, MOBILITY
		.group_visual, .group_hearing, .group_mobility{

			display: none;

			padding: 10px;

			// VISUAL, HEARING, MOBILITY title
			.description{
				text-transform: uppercase;
				border-bottom: 1px solid $border-color;
				font-weight: bold;
				@include h3;
			}

			div.field-type-number-float.shade{
				background: #F2F2F2;
				border-top: 1px solid $border-color;
				border-bottom: 1px solid $border-color;	
			}

			div.rate-field-label{
				float: left;
				line-height: 33px;
			}
		} //.group_visual, .group_hearing, .group_mobility

		/* Rate */
		.rate-widget-emotion {


			// Buttons
			.item-list{
				display: none;

				ul{
					margin: 0;
					padding-top: 4px;
					float: right;


					li{

					}
				}

				a{

					&.rate-button{
						height: 24px;
						width: 24px;		
						line-height: 24px;
						display: block;
						padding: 0;
						margin: 0 5px;
						text-align: center;
						text-decoration: none;	
						border-color: #ccc;
						background-color: #ddd;
						color: #666 !important;	
					}
					&.up{
						@include replace-text("../images/thumbs_up.gif", 0, 0);
						border: none;

						&:hover, &.active, &.selected{
							@include replace-text("../images/thumbs_up.gif", 0, -24px);
						}
					}

					&.down{
						@include replace-text("../images/thumbs_down.gif", 0, 0);
						border: none;

						&:hover, &.active, &.selected{
							@include replace-text("../images/thumbs_down.gif", 0, -24px);
						}
					}

					&.na{
						&:hover, &.active, &.selected{
							border-color: #1083d1;
							background-color: #BBE3F4;
							color: #1083d1 !important;
						}
					}
				}

			}

		} //.rate-widget-emotion


		// Percentage bar
		.group_visual .result{
			background: url(../images/icon_visual.gif) left center no-repeat;
		}
		.group_hearing .result{
			background: url(../images/icon_hearing.gif) left center no-repeat;
		}
		.group_mobility .result{
			background: url(../images/icon_mobility.gif) left center no-repeat;
		}
		
		.rate-info{
			display: none;
			display: inline;
			
			.result {

				float: right;
				width: 195px;
				line-height: 33px;
				padding-left: 25px;

				.percentage-bar {
					@include inline-block;
					background: #c00;
					width: 100px;
					height: 10px;

					&.empty{
						background-color: $border-color;
					}

					div.good{
						display: block;
						background: $green;	
						height: 10px;
					}
				}
			}
		}	//.rate-info
	
		//Location teaser
		&.node-teaser{
			@include clearfix;
			
			//category icon
			.category-icon{
				position: absolute;
			}
			
			.group-teaser-left{
				width: 250px;
				float: left;
				margin-left: 40px;
				margin-right: 10px;
				
				.field-name-field-title{
					@include h3;
					text-transform: capitalize;
					margin-top: 0px;
					margin-bottom: 0;
					font-weight: bold;
				}
				
				.field-name-field-subheading{
					line-height: 1.5em;
					
				}
				
				.field-name-field-address{
					font-size: .8em;
				}
			}
			
			.group-teaser-right {
				float: right;
				width: 262px;
				
				
				.rate-info{
					display: block;
					@include clearfix;
				}
				
				span.label{
					font-weight: bold;
					float: left;
					line-height: 33px;
					padding-left: 25px;
					
					&.visual{
						background: url(../images/icon_visual.gif) left center no-repeat;
					}
					&.hearing{
						background: url(../images/icon_hearing.gif) left center no-repeat;
					}
					&.mobility{
						background: url(../images/icon_mobility.gif) left center no-repeat;
					}
					
				}
				
				.result{
					padding-left: 0;
					width: auto;
				}
			}
		} //&.node-teaser
		
	} // & .node-location
	
	
	//Location submit form
	#location-node-form {
		padding: 0 10px;
		
		#edit-title{
		}
		
		#edit-field-address {
		
		}
		
		#edit-field-city, #edit-field-postcode{
			float: left;
			clear: left;
			input {
				width: 195px;
			}			
		}
		
		#edit-field-state, #edit-field-country{
			float: left;
			margin-left: 20px;
			input, select {
				width: 195px;
			}
			
			
		}
		
		.map {
			position: relative;
			clear: left;
			
			label{
				
				background-color: rgba(0, 0, 0, 0.66);
				color: #fff;
				font-weight: normal;
				padding: 2px 5px;
				width: 360px;
				@include border-radius(3px);
				position: absolute;
				top: 3px;
				left: 30px;
				z-index: 40;
			}
			
		}
		
		#create-location-map{
			clear: left;
		}
		
		.vertical-tabs{
			display: none;
		}
		
		#edit-field-latitude,
		#edit-field-longitude{
			display: none;
		}
		
	} //#location-node-form 


	//User login
	.rpx-links{
		border-bottom: 1px solid $border-color;
		
		span.ext{
			display: none;
		}
		
		// hide link "Sign in using one of these accounts:"
		br,
		a.rpxnow.ext:first-child{
			display: none;
		}
		

	}


	//User Registration
	#user-register-form{
		#field-user-city-add-more-wrapper{
			.descriptions{
				font-size: 10px;
				line-height: 12px;
				
				&.error{
					background: none;
				}
			}
		}
	}
	
	//User Profile
	div.profile{
		padding: 0 10px;
		@include clearfix;
		
		.field-name-field-user-follow,
		.field-name-field-user-edit{
			position: absolute;
			top: 2px;
			right: 0;
			
			a.button{
				@include title-button;	
				
			}
			
			//Flag
			.flag-wrapper{
				a.flag{
					@include title-button;
					&.unflag-action{
						background: #efefef none;
					  	border-color: #efefef;
					  	text-shadow: none;
					  	color: #aaa;
					}

				}
				
				&.flag-waiting {
					.flag-throbber {
						background: none;
						padding: 0;
					}
					
					a.flag{
						background: #666;
						color: white;
						border-color: #555;
						text-shadow: #222 0 -1px 0;
						opacity: 1;
					  	filter: alpha(opacity=100); /* IE */
					}
					
				}
				
				.flag-message{
					border: 1px solid #ed5;
					color: #840;
					background-color: #fffce5;
					padding: 5px 10px;
					width: 150px;
					right: 0;
					top: 2.1em;
					left: auto;
					
					&.flag-unflagged-message{
						width: auto;
					}
				}
				
			}


			
		}

		.user-picture{
			float: left;
		}
		
		.group_profile{
		
			.field-name-field-name{
				@include h2;
				color: $green;
				font-weight: bold;
				margin: 0;
			}
			
			.field-name-field-user-city{
				margin-bottom: 1em;
			}
			
			.field-name-field-bio{
				clear: left;
			}			
			
		}
		
		
		
	} //div.profile
	
	
	// Views
	.view{
		//My Reviews, search view
		&.view-id-user_reviews, 
		&.view-id-search,
		&.view-taxonomy-term{
			.views-row{
				padding: 10px;
				border-bottom: 1px solid $border-color;
			}
		}
		
		// Search review Page
		&.view-display-id-page{
			padding: 0;
			

		}//&.view-id-search
		
		// Create review Page
		&.view-display-id-page_2{
			padding: 0;
		}
		
	} //.view
	
	// Search review Block
	#block-views-exp-search-page{
		#edit-field-search-value{
			width: 500px;
		}
		
		#edit-field-category-tid-wrapper{
			clear: left;
		}

		h6{
			display: block;
			float: left;
			width: 100%;
			clear: both;
			margin: 0;
			margin-top: 1em;
		}
		
		// Latitude & Longitude
		#edit-distance-wrapper{
			display: none;
		}
		
		// All sucategory filters 
		#edit-delta-wrapper, #edit-delta-1-wrapper, #edit-delta-4-wrapper, #edit-delta-15-wrapper, #edit-delta-7-wrapper, 
		#edit-delta-16-wrapper, #edit-delta-8-wrapper, #edit-delta-9-wrapper, #edit-delta-11-wrapper, #edit-delta-12-wrapper, 
		#edit-delta-13-wrapper, #edit-delta-10-wrapper, #edit-delta-14-wrapper {
			display: none; 
		}
		
		// visual, hearing & mobility
		#edit-field-widget1-value-wrapper, #edit-field-widget5-value-wrapper, #edit-field-widget9-value-wrapper, 
		#edit-field-widget12-value-wrapper, #edit-field-widget2-value-wrapper, #edit-field-widget6-value-wrapper, 
		#edit-field-widget10-value-wrapper, #edit-field-widget13-value-wrapper, #edit-field-widget3-value-wrapper,
		#edit-field-widget7-value-wrapper, #edit-field-widget11-value-wrapper, #edit-field-widget14-value-wrapper, #edit-field-widget4-value-wrapper, 
		#edit-field-widget8-value-wrapper {
			display: none; 
			float: none;
			width: 296px;
		}
	
		.checkboxFilter{
			&.visual{
				
			}
			&.visual, &.hearing, &.mobility{
				float: left;
				width: 296px;
			}
			&.mobility{
				clear: left;
				margin-bottom: 1em;
			}
		}

		
		.views-submit-button{
			clear: left;
			#edit-submit-search{
				display: none;
			}			
		}
		
		// Near
		#filter-location-wrapper{
			.descriptions{
				font-size: 10px;
				position: absolute;
				line-height: 12px;
				
				&.error{
					background: none;
				}
			}
		}
		
	}//	#block-views-exp-search-page
	
	//Create Review Block
	#block-views-exp-search-page-2{
	
		#edit-field-search-value{
			width: 500px;
		}
	
		// Latitude & Longitude
		#edit-distance-wrapper{
			display: none;
		}
		
		// Near
		#filter-location-wrapper{
			.descriptions{
				font-size: 10px;
				position: absolute;
				line-height: 12px;
				
				&.error{
					background: none;
				}
			}
		}
		// Submit
		.views-submit-button{
			padding: 0;
			#edit-submit-search{
				display: none;
			}
			
			a.button{
